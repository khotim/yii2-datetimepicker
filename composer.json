{
    "name": "khotim/yii2-datetimepicker",
    "description": "The DatetimePicker extension for the Yii framework",
    "keywords": ["yii2", "Jquery UI", "DatePicker", "TimePicker", "DatetimePicker"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-datetimepicker/issues",
        "source": "https://github.com/khotim/yii2-datetimepicker"
    },
    "authors": [
        {
            "name": "Thimy Khotim",
            "email": "thimy.khotim@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.4",
        "yiisoft/yii2-jui": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "khotim\\datetime\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
